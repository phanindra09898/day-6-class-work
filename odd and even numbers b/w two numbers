#include <stdio.h>

int main() {
    int num1, num2, i;
    
    printf("Enter the first number: ");
    scanf("%d", &num1);
    
    printf("Enter the second number: ");
    scanf("%d", &num2);
    
    printf("Even numbers between %d and %d: ", num1, num2);
    for (i = num1; i <= num2; i++) {
        if (i % 2 == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
    
    printf("Odd numbers between %d and %d: ", num1, num2);
    for (i = num1; i <= num2; i++) {
        if (i % 2 != 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
    
    return 0;
}

                 
